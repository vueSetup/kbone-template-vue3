{
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "clean": "pnpm clean:packages & rimraf node_modules",
    "commit": "git-cz",
    "start": "pnpm --filter kbone-template-vue3 start",
    "start:mp": "pnpm --filter kbone-template-vue3 start:mp",
    "build": "pnpm --filter kbone-template-vue3 build"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
